{
  "name": "@wrikka/vue-design-system",
  "version": "1.0.0",
  "type": "module",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": "./index.ts",
    "./components/*": "./app/components/*/index.ts",
    "./composables/*": "./app/composables/*.ts",
    "./utils/*": "./app/utils/*.ts",
    "./types/*": "./types/*.ts"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "vue-tsc --noEmit && biome lint --write",
    "format": "biome format --write .",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "generate-meta": "bun run scripts/generate-component-meta.ts",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@nuxtjs/color-mode": "3.5.2",
    "@shikijs/langs": "^3.14.0",
    "@shikijs/markdown-it": "^3.14.0",
    "@shikijs/monaco": "^3.14.0",
    "@shikijs/themes": "^3.14.0",
    "@types/animejs": "^3.1.13",
    "@types/node": "^24.9.2",
    "@types/turndown": "^5.0.6",
    "@vueuse/core": "^14.0.0",
    "@vueuse/nuxt": "14.0.0",
    "animejs": "^4.2.2",
    "gray-matter": "^4.0.3",
    "monaco-editor-core": "^0.54.0",
    "nuxt": "^4.2.0",
    "nuxt-auth-utils": "^0.5.25",
    "shiki": "^3.14.0",
    "turndown": "^7.2.2",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.2.10",
    "@iconify-json/mdi": "^1.2.3",
    "@nuxt/test-utils": "^3.20.1",
    "@testing-library/vue": "^8.1.0",
    "@types/markdown-it": "^14.1.2",
    "@unocss/nuxt": "^66.5.4",
    "@vitest/ui": "4.0.5",
    "@vue/test-utils": "^2.4.6",
    "chokidar": "^4.0.3",
    "happy-dom": "20.0.10",
    "markdown-it": "^14.1.0",
    "nuxt-mcp": "^0.2.5",
    "sonda": "^0.9.0",
    "tsdown": "^0.15.12",
    "unocss": "^66.5.4",
    "vite": "^7.1.12",
    "vite-plugin-checker": "^0.11.0",
    "vitest": "4.0.5",
    "vue-tsc": "^3.1.2"
  }
}
